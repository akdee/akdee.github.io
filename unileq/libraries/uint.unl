#Comparisons

uint.cmp:
	#Call  : 0 ? uint.cmp a b lt eq gt
	#Effect:
	#     if [a]<[b]: goto lt
	#     if [a]=[b]: goto eq
	#     if [a]>[b]: goto gt
	.p0 .p0   ?+1
	.p0 0     ?+1
	.p0 .z-2  ?+1
	.p2 .p2   ?+1
	.p2 0     ?+1
	.p2 .z-3  ?+1
	0   0     ?+1
	.r0 .z-1  ?+1
	#get [a]
.ret:
	.z  .p0:0 ?+1
	.p1 .p1   ?+1
	.p1 .z    ?+1
	#r0 used when on return pass.
	.z  .z    .r0:?+1
	.b
	.b  .p1:0 ?+1
	.a  .a    ?+1
	.a  .b    ?+1
	.b  .b    ?+1
	#get [b]
	.z  .p2:0 ?+1
	.b  .z    ?+1
	.z  .z    ?+1
	#[a]-[b]
	.r0 .z+1  ?+1
	.a  .b:0  .le
	#[a]>[b]
	.p0 .z-4  .ret
.le:
	.a  .z    .eq
	#[a]<[b]
	.p0 .z-2  .ret
.eq:
	#[a]=[b]
	.p0 .z-3  .ret
.a:0
0-4 0-3 0-2 0-1 .z:0 1

