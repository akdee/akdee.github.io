	#Effect: [r]=-[a]
	#Setup new stack.
	.tmp  .tmp  ?+1
	.tmp  .p0   ?+1
	0     .off  ?+1
	0     .tmp  ?+1
	.p0   0     ?+1
	.p2   0     ?+1
	.ret  0     ?+1
	.val  .val  ?+1
	#Get [r]
	.tmp  .tmp  ?+1
	.tmp  .p0:0 ?+1
	.p1   .p1   ?+1
	.p1   .tmp  ?+1
	.p6   .p6   ?+1
	.p6   .tmp  ?+1
	.val  .p1:0 ?+1 #[pos]=-[r]
	#Get [a]
	.tmp  .tmp  ?+1
	.tmp  .p2:1 ?+1
	.p3   .p3   ?+1
	.p3   .tmp  ?+1
	.val  .p3:0 ?+1 #[neg]=-[a]
	#ret
	.tmp  .tmp  ?+1
	.tmp  .val  ?+1
	.p6:0 .tmp  ?+1 #[r]=[r]-([r]-[a]-[b])=[a]+[b]
	0     0     .ret:2
.off:2
.val:0
.tmp:0

