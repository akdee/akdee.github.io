	.tmp  .a2:2 ?+1
	.p0   .p0   ?+1
	.p0   .tmp  ?+1
	.p1   .p1   ?+1
	.p1   .tmp  ?+1
	.pos  .p0:0 ?+1  #[pos]=-[r]
	#Get [a]
