uint.and:
	#Call  : 0 ? uint.and r a b
	#Effect: [r]=[a]&[b]
	#Setup stack pointer.
	.at   .at   ?+1
	.at   .a0   ?+1
	0     .at   ?+1
	.a0   0     ?+1
	.a2   0     ?+1
	.a3   0     ?+1
	.a4   0     ?+1
	.a5   0     ?+1
	#Get [r]
	.at   .at   ?+1
	.at   .a2:2 ?+1
	.r0   .r0   ?+1
	.r0   .at   ?+1
	.r1   .r1   ?+1
	.r1   .at   ?+1
	#Get [a]
	.at   .at   ?+1
	.at   .a3:3 ?+1
	.p0   .p0   ?+1
	.p0   .at   ?+1
	.a    .a    ?+1
	.a    .z+1  ?+1
	.a    .p0:0 ?+1
	#Get [b]
	.at   .at   ?+1
	.at   .a4:4 ?+1
